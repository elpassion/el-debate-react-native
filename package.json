{
  "name": "eldebate",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-preset-react-native": "^4.0.0",
    "chai": "^4.1.2",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "enzyme-to-json": "^3.1.2",
    "jest": "^21.2.1",
    "jest-expo": "~19.0.0",
    "mocha": "^4.0.1",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.0.0",
    "react-native-dotenv": "^0.1.0",
    "react-native-mock": "^0.3.1",
    "react-native-scripts": "1.1.0",
    "react-test-renderer": "^16.0.0-alpha.12"
  },
  "rnpm": {
    "resources": [
      "./resources/fonts"
    ]
  },
  "main": "./node_modules/react-native-scripts/build/bin/crna-entry.js",
  "scripts": {
    "test": "jest"
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.js",
      "**/?(*.)(spec|test).js?(x)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/__tests__/setup/"
    ],
    "setupFiles": [
      "./__tests__/setup/Setup.js"
    ],
    "preset": "react-native",
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|react-navigation)/"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "dependencies": {
    "apsl-react-native-button": "^3.1.0",
    "expo": "^19.0.1",
    "react": "16.0.0-alpha.12",
    "react-native": "^0.46.1",
    "react-native-modalbox": "^1.4.2",
    "react-native-navbar": "^1.8.0",
    "react-native-navigation-bar": "git+https://github.com/jestemkarol/react-native-navigation-bar.git",
    "react-navigation": "^1.0.0-beta.11",
    "react-router-native": "^4.1.1"
  }
}
